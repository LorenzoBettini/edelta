  <arcs xsi:type="petrinet:PTArc"
      dst="//@transitions.3"/>
  <arcs xsi:type="petrinet:PTArc"
      dst="//@transitions.1"/>
  <arcs xsi:type="petrinet:PTArc"
      dst="//@transitions.2"/>
  <arcs xsi:type="petrinet:TPArc"
      dst="//@places.1"/>
  <arcs xsi:type="petrinet:TPArc"
      dst="//@places.2"/>
  <arcs xsi:type="petrinet:TPArc"
      dst="//@places.0"/>
