<?xml version="1.0" encoding="UTF-8"?>
<petrinet:Petrinet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrinet="http://gssi.it/petrinet"
    xsi:schemaLocation="http://gssi.it/petrinet Petrinet.ecore"
